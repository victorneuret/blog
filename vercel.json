{
  "headers": [
    {
      "source": "/rss.xml*",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        },
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, max-age=0"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Content-Disposition",
          "value": "inline; filename=rss.xml"
        }
      ]
    }
  ]
}
